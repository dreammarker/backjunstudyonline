import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		int a,cnt=0,sum=0;
		sum=scanner.nextInt();
		a=sum;
		if(a<=99&a>=0) {
			do {
			sum=(sum%10*10)+(sum%10+sum/10)%10;
			cnt++;
			}while(sum!=a);
			System.out.println(cnt);
			}
			scanner.close();
	}
}
